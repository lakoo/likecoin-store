{
  "name": "likecoin-store",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "William Chong <williamchong007@msn.com>",
  "private": true,
  "scripts": {
    "dev": "IS_TESTNET=TRUE backpack dev",
    "build": "nuxt build && backpack build",
    "build_testnet": "IS_TESTNET=TRUE nuxt build && npm run build_server_testnet",
    "build_testnet_server": "IS_TESTNET=TRUE backpack build",
    "start": "cross-env NODE_ENV=production node build/main.js",
    "precommit": "npm run lint",
    "lint": "eslint --ext .js,.vue .",
    "prepare": "cd node_modules/web3-core-requestmanager/src && sed -i.bak \"/var global = Function('return this')();/d\" givenProvider.js && rm givenProvider.js.bak",
    "test:express": "ava -v test/express.test.js",
    "test:nuxt": "ava -v test/nuxt.test.js",
    "test:e2e": "./node_modules/.bin/nightwatch --config test/e2e/nightwatch.conf.js"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "@google-cloud/pubsub": "^0.17.0",
    "@ledgerhq/hw-app-eth": "^4.7.3",
    "@ledgerhq/hw-transport-u2f": "^4.7.3",
    "@ledgerhq/web3-subprovider": "^4.7.3",
    "@likecoin/ethereum-blockies": "^0.1.1",
    "@likecoin/likecoin-email-templates": "^1.0.0",
    "@likecoin/vue-image-lightbox": "^5.5.3",
    "@nuxtjs/google-analytics": "^2.0.2",
    "@nuxtjs/google-tag-manager": "^1.1.0",
    "abi-decoder": "^1.0.9",
    "aws-sdk": "^2.193.0",
    "axios": "^0.18.0",
    "bignumber.js": "^6.0.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cross-env": "^5.0.1",
    "eth-sig-util": "^1.4.2",
    "express": "^4.15.3",
    "firebase-admin": "^5.7.0",
    "i18n": "^0.8.3",
    "image-type": "^3.0.0",
    "js-sha256": "^0.9.0",
    "moment": "^2.21.0",
    "multer": "^1.3.0",
    "nuxt": "^1.3.0",
    "sharp": "^0.20.0",
    "source-map-support": "^0.5.3",
    "stripe": "^5.6.1",
    "url-parse": "^1.2.0",
    "uuid": "^3.2.1",
    "vue-awesome-swiper": "^3.1.2",
    "vue-i18n": "^7.4.2",
    "vue-intercom": "^1.0.12",
    "vue-lazyload": "^1.2.1",
    "vue-material": "^1.0.0-beta-8",
    "vue-stripe-checkout": "^1.0.3",
    "vuex": "^3.0.1",
    "web3": "^1.0.0-beta.28",
    "web3-provider-engine": "^13.6.6"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-eslint": "^8.2.1",
    "backpack-core": "^0.7.0",
    "chromedriver": "^2.36.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "nightwatch": "^0.9.20",
